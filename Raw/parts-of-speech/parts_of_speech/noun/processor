with open('noun 2.txt', 'r') as f:
    n2=f.readlines()
    
with open('noun 1.txt', 'r') as f:
    n1=f.readlines()


n1p=[]
n2p=[]

for nx in n1:
    nx=nx.strip()
    nx=nx.lower()
    n1p.append(nx)

for ny in n2:
    ny=ny.strip()
    ny=ny.lower()
    n2p.append(ny)

print("n1",len(n1p))
print("n2",len(n2p))
for n in n1p:
    if n not in n2p:
        n2p.append(n)

print("n2 m",len(n2p))

with open('noun.txt', 'w') as f:
    for item in n2p:
        f.write("%s\n" % item)